{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "JLink Debug",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/stm32evk.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "openocd",
            "configFiles": ["${workspaceFolder}/configs/stlink.cfg", "${workspaceFolder}/configs/stm32f0x.cfg"],
            "device": "STM32F030K6",
            "interface": "swd",
            "preLaunchTask": "build",
            "showDevDebugOutput": "raw",
        },
        {
            "name": "JLink Debug (attach)",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/stm32evk.elf",
            "request": "attach",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "openocd",
            "configFiles": ["/usr/share/openocd/scripts/interface/stlink-v2.cfg", "/usr/share/openocd/scripts/target/stm32f0x.cfg"],
            "device": "STM32F030K6",
            "interface": "swd",            
            "preLaunchTask": "build",
            "showDevDebugOutput": "raw",
        }
    ]
}